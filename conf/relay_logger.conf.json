{
    "tags": [],
    "filterOut": ["verbose", "info", "debug"],
    "targets": { "default":
    {
        "file": "/wigwag/log/devicejs.log",
        "format":
        { "time": "[%ld:%03d] ",
            "level": "%-10s ",    // use some fancy printf stuff to keep the columns straight
            "tag": "\u001b[33m%-10s\u001b[39m ",
            "origin": "\u001b[37m\u001b[100m%-10s\u001b[39m\u001b[49m " },
        "filters":
            [
                { "target": "default",
                    "mask": "debug",
                    "pre": "\u001b[36m",
                    "post": "\u001b[39m" },
                { "target": "default",
                    "mask": "verbose",
                    "pre": "\u001b[33m",
                    "post": "\u001b[39m" },
                { "target": "default",
                    "mask": "info",
                    "pre": "\u001b[33m",
                    "post": "\u001b[39m" },
                { "target": "default",
                    "mask": "trace",
                    "pre": "\u001b[90m",
                    "post": "\u001b[39m" },
                { "target": "default",
                    "mask": "warn",
                    "pre": "\u001b[33m",
                    "post": "\u001b[39m" },
                { "target": "default",
                    "mask": "error",
                    "pre": "\u001b[31m",
                    "post":"\u001b[39m" },
                { "target": "default",
                    "mask": "success",
                    "pre": "\u001b[32m",
                    "post": "\u001b[39m" },
                { "target": "default", "mask": "log", "pre": "\u001b[39m" },
                // let's do special stuff for the stdout and stderr tags
                { "mask": "log",
                    "tag": "stdout",
                    "post_fmt_pre_msg": "\u001b[90m[console] \u001b[39m" }, // place this before the actual log "message"
                { "tag": "stderr",
                    "mask": "error",
                    "post_fmt_pre_msg": "\u001b[90m[console] \u001b[31m",
                    "post": "\u001b[39m" } ]
    }
}
}
